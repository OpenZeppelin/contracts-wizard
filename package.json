{
  "name": "@openzeppelin/wizard",
  "version": "0.1.0",
  "description": "A boilerplate generator to get started with OpenZeppelin Contracts",
  "repository": "github:OpenZeppelin/wizard",
  "main": "dist/index.js",
  "files": [
    "/dist",
    "!*.test.{js,d.ts}{,.map}"
  ],
  "scripts": {
    "prepare": "tsc",
    "prepublish": "rimraf dist",
    "test": "tsc && ava",
    "test:watch": "fgbg 'ava --watch' 'tsc --watch'"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^10.17.51",
    "ava": "^3.15.0",
    "fgbg": "^0.1.5",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "license": "MIT",
  "dependencies": {
    "array.prototype.flatmap": "^1.2.4"
  }
}
