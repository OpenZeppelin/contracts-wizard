{
  "name": "@ericglau/wizard-mcp",
  "version": "0.0.1-alpha.5",
  "description": "MCP Server for OpenZeppelin Contracts Wizard",
  "license": "AGPL-3.0-only",
  "repository": "https://github.com/ericglau/contracts-wizard",
  "main": "dist/index.js",
  "bin": {
    "wizard-mcp": "dist/index.js"
  },
  "ts:main": "src/index.ts",
  "files": [
    "LICENSE",
    "NOTICE",
    "/dist",
    "/src",
    "!**/*.test.*"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector@0.12.0 node dist/index.js",
    "test": "ava",
    "test:update-snapshots": "ava --update-snapshots",
    "test:watch": "ava --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.1",
    "@openzeppelin/wizard": "^0.5.6",
    "@ericglau/wizard-common": "^0.0.1-alpha.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ava": "^6.0.0",
    "ts-node": "^10.4.0"
  }
}
