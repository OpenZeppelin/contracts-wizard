    "test:watch": "ava --watch"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/uniswap-hooks": "github:OpenZeppelin/uniswap-hooks#b9eecc8fbb0c5b381121f0025f4389415010b818",
    "@types/node": "^20.0.0",
    "@types/semver": "^7.5.7",
    "@uniswap/v4-core": "^1.0.2",
    "@uniswap/v4-periphery": "^1.0.3",
    "ava": "^6.0.0",
    "hardhat": "^2.22.11",
    "rimraf": "^5.0.0",
    "semver": "^7.6.0",
    "solidity-ast": "^0.4.18",
    "ts-node": "^10.4.0",
    "typescript": "^5.0.0"
  }
}
